{
  "name": "@hawk-tracker/plugin-performance",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "description": "Performance monitoring plugin for the web SDK",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
